# ignore all files
*
# but include directories
!*/

# git
!/**/.gitkeep
!/.gitignore

# github
!/.github/**

# idea
!/.idea/runConfigurations/**

# java
!/**/src/**
# maven
!/**/pom.xml
# gradle
!/build.gradle
!/gradle.properties
!/gradle/wrapper/gradle-wrapper.properties
!/gradlew
!/gradlew.bat
!/settings.gradle

# docker
!/**/Dockerfile

# misc
!/**/*.md
!/**/.editorconfig
!/LICENSE
